{
  "info": {
    "title": "Holiday Promo API",
    "version": "1.0.0",
    "description": "API for the Holiday Promo application. Provides user management, health checks, and authentication endpoints."
  },
  "servers": [
    {
      "url": "http://localhost:3000/api/rpc",
      "description": "Local development server"
    },
    {
      "url": "https://your-production-domain.com/api/rpc",
      "description": "Production server"
    }
  ],
  "openapi": "3.1.1",
  "paths": {
    "/healthCheck": {
      "post": {
        "operationId": "healthCheck",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {},
                    {
                      "not": {}
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/healthCheckLog/log": {
      "post": {
        "operationId": "healthCheckLog.log",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "enum": ["connected", "disconnected"]
                  },
                  "error": {
                    "type": "string"
                  }
                },
                "required": ["status"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {},
                    {
                      "not": {}
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/privateData": {
      "post": {
        "operationId": "privateData",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {},
                    {
                      "not": {}
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/user/me": {
      "post": {
        "operationId": "user.me",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {},
                    {
                      "not": {}
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/user/list": {
      "post": {
        "operationId": "user.list",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {},
                    {
                      "not": {}
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/user/delete": {
      "post": {
        "operationId": "user.delete",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {},
                    {
                      "not": {}
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/user/updateProfile": {
      "post": {
        "operationId": "user.updateProfile",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "minLength": 2,
                    "maxLength": 100
                  },
                  "email": {
                    "type": "string",
                    "format": "email"
                  }
                },
                "required": ["name", "email"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {},
                    {
                      "not": {}
                    }
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}
